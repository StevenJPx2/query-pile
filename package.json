{
  "name": "query-pile",
  "version": "0.0.2",
  "description": "A set of utilities to make using Tanstack/React Query easier.",
  "repository": "StevenJPx2/query-pile",
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "keywords": [
    "react",
    "query",
    "tanstack",
    "react-query",
    "hooks",
    "react-hooks"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "vitest dev",
    "lint": "eslint --cache --ext .ts,.js,.mjs,.cjs . && prettier -c src test",
    "lint:fix": "eslint --cache --ext .ts,.js,.mjs,.cjs . --fix && prettier -c src test -w",
    "prepack": "bun run build",
    "play": "jiti playground",
    "release": "bun run test && changelogen --release && npm publish && git push --follow-tags",
    "test": "bun run lint && bun run test:types && vitest run --coverage",
    "test:types": "tsc --noEmit --skipLibCheck"
  },
  "peerDependencies": {
    "typescript": ">=5",
    "@tanstack/react-query": ">=5.20 || <6"
  },
  "devDependencies": {
    "@tanstack/react-query": "^5.20.2",
    "@types/node": "^20.11.17",
    "@vitest/coverage-v8": "^1.2.2",
    "changelogen": "^0.5.5",
    "eslint": "^8.56.0",
    "eslint-config-unjs": "^0.2.1",
    "jiti": "^1.21.0",
    "prettier": "^3.2.5",
    "unbuild": "^2.0.0",
    "vitest": "^1.2.2"
  }
}